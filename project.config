[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Ben (bgm13)
[access "refs/for/refs/*"]
	read = group user/Ben (bgm13)
	push = group user/Ben (bgm13)
	label-Code-Review = -2..+2 group user/Ben (bgm13)
	label-Verified = -1..+1 group user/Ben (bgm13)
	rebase = group user/Ben (bgm13)
	removeReviewer = group user/Ben (bgm13)
	submit = group user/Ben (bgm13)
[access "refs/heads/*"]
	read = group user/Ben (bgm13)
	create = group user/Ben (bgm13)
	push = +force group user/Ben (bgm13)
	pushMerge = group user/Ben (bgm13)
[access "refs/tags/*"]
	push = +force group user/Ben (bgm13)
	pushTag = group user/Ben (bgm13)
	pushSignedTag = group user/Ben (bgm13)
